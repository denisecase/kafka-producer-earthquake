# Kafka Provider
KAFKA_PROVIDER=railway

# Railway Kafka Broker (Set in Railway Dashboard)
KAFKA_BROKER=${KAFKA_BROKER}

# Kafka Topic
KAFKA_TOPIC=earthquake-events

# Consumer Group ID
KAFKA_CONSUMER_GROUP=earthquake-railway-group

# Security Config (if Railway requires authentication)
KAFKA_SECURITY_PROTOCOL=SASL_SSL
KAFKA_SASL_MECHANISM=PLAIN
KAFKA_API_KEY=${KAFKA_API_KEY}
KAFKA_API_SECRET=${KAFKA_API_SECRET}

# Log Level
LOG_LEVEL=INFO
