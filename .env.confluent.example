# Kafka Provider
KAFKA_PROVIDER=confluent

# Kafka Broker (Confluent Cloud)
KAFKA_BROKER=<your-confluent-cluster>.confluent.cloud:9092

# Kafka API Keys (replace with your own)
KAFKA_API_KEY=<your-confluent-api-key>
KAFKA_API_SECRET=<your-confluent-api-secret>

# Kafka Topic
KAFKA_TOPIC=earthquake-events

# Consumer Group ID
KAFKA_CONSUMER_GROUP=earthquake-confluent-group

# Security Protocol & SSL Config (needed for Confluent)
KAFKA_SECURITY_PROTOCOL=SASL_SSL
KAFKA_SASL_MECHANISM=PLAIN

# Log Level
LOG_LEVEL=INFO
